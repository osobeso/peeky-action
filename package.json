{
  "name": "peeky-action",
  "version": "0.0.0",
  "private": true,
  "description": "Peeky",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "eslint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "pack": "ncc build",
    "pre-release": "npm prune --production",
    "all": "npm i && npm run build && npm run format && npm run eslint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osobeso/intellicode-team-model.git"
  },
  "keywords": [
    "peeky",
    "action"
  ],
  "author": "Oscar Obeso",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.2",
    "peeky": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.17.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.19.2",
    "@typescript-eslint/parser": "^2.8.0",
    "typescript": "^3.6.4",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.0"
  }
}
